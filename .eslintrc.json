{
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "react/react-in-jsx-scope": "off"
  },
  "parserOptions": {
    "ecmaVersion": "latest"
  },

  "env": {
    "es6": true
  }
}
